<html>
<head>
<style type="text/css">
table {  border-collapse:collapse;  }
table, td, th  {  border:1px solid black;  }
</style>

</head>
<body>
<table>
  <tr>
    	<th>id</th>
    	<th>booth</th>
    	<th>isSpecialName</th>
    	<th>brandId</th>
    </tr>
#foreach( $shop in $shops )
    <tr>
    	<td>$shop.id</td>
    	<td>$shop.booth</td>
    	<td>$shop.isSpecialName</td>
    	<td><a href="$rc.contextPath/brands/$shop.brandId">$shop.brandId</a></td>
    </tr>
#end
</table>
</body>
</html>